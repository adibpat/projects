#include <stdio.h>
#include <stdlib.h>

typedef struct node {
    int data;
    struct node *next;
} node;

node* newNode(int data) {
    node *temp = malloc(1, sizeof(node));
    temp->next = NULL;
    temp->data = data;
    return (temp);
}

int get_ll_length (node *head) {
    int count = 0;
    while(head != NULL) {
        count++;
        head = head->next;
    }
    return (count);
}

int main () {


    node *head = newNode(1);
    head->next = newNode(2);
    head->next->next = newNode(3);
    
    return (0);
}
