#include <stdio.h>

struct node {
    int data;
    struct node *left;
    struct node *right;
    struct node *parent;
} node;

node *newNode (node *root, int data) {
    node *new_node = calloc(1, sizeof(node));
    new_node->data = data;
    new_node->left = NULL;
    new_node->right = NULL;
    new_node->parent = NULL;
    return (new_node);
}

void inorder_print(node *root) {

    if (root == NULL) return;

    inorder_print(root->left);
    printf("%d ", root->data);
    inorder_print(root->right);
}

int main () {

    /* Problem 1 - build123()*/
    node* root;
    root = new_node(2);
    root->left = new_node(1);
    root->right = new_node(3);
    inorder_print(root);

    return (0);
}
